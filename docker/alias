alias dac='docker container rm -f $(docker container ls -a -q)'
alias dai='docker image rm -f $(docker image ls -q)'
alias dbi='docker image build -t '
alias dc='docker container ls -a'
alias di='docker image ls '
alias ip='docker image prune '
alias ii='docker image inspect '
alias iv='docker inspect volume'
alias nl='docker network ls'
alias vl='docker volume ls'
alias cn='docker network create -d bridge --subnet "10.0.0.0/24"'
alias in='docker inspect network'
alias dav='docker volume rm -f $(docker volume ls -q)'